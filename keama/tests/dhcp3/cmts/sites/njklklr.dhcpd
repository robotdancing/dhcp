# DHCPD njklklr Config File 10/14/2010 
shared-network njklklr {
    authoritative;

    subnet 173.220.142.98 netmask 255.255.255.255 {
    }



    ## Cable Modem Subnet - 
    subnet 10.160.24.0 netmask 255.255.255.0 {
        ### General Options
        option subnet-mask 255.255.255.0;
        option broadcast-address 10.160.24.255;
        option domain-name "pavlovmedia.net";
        server-identifier 68.234.128.73;
        next-server 68.234.128.73;
        option routers 10.160.24.1;
        option time-servers 10.160.24.1;
        option log-servers 10.160.24.1;
        option time-offset -18000;
    
        pool {
            allow members of "cm-unregistered";

            filename "standard.cm";

            default-lease-time 2400;
            max-lease-time 2400;
            range 10.160.24.20 10.160.24.250;
        }
    }

#include "dhcp3/cmts/host_includes/njklklr.hosts";

#  OUTLIERS


    subnet 10.160.26.0 netmask 255.255.255.0 {
        option routers 10.160.26.1;
        option subnet-mask 255.255.255.0;
        option broadcast-address 10.160.26.255;
        option domain-name "client.pavlovmedia.net";
        option netbios-name-servers 66.253.230.210;
        option domain-name-servers 66.253.214.16, 50.30.184.16;
#        default-lease-time dTlease0;
#        max-lease-time mdTlease0;
        pool {
            deny members of "ruckus";
            deny members of "cm-unregistered";
            deny members of "3010s";
            range 10.160.26.20 10.160.26.250;
            default-lease-time 2400;
            max-lease-time 2400;
        }
    }
}
