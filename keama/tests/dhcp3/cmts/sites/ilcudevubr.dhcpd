# DHCPD ilcudevubr Config File 7/17/2010 
shared-network ilcudevubr {
    authoritative;

    subnet 66.253.201.135 netmask 255.255.255.255 {
    }


   ## Cable Modem Subnet - 10.160.248.0/22
    subnet 10.160.248.0 netmask 255.255.252.0 {
        ### General Options
        option subnet-mask 255.255.252.0;
        option broadcast-address 10.160.251.255;
        option domain-name "pavlovmedia.net";
        #server-identifier 66.253.253.36;
        #next-server 66.253.253.36;
	server-identifier 68.234.128.73;
        next-server 68.234.128.73;
        option routers 10.160.248.1;
        option time-servers 10.160.248.1;
        option log-servers 10.160.248.1;
        option time-offset -18000;
    
        pool {
            allow members of "cm-unregistered";
#            deny known clients;
#            deny members of "ruckus";
#            deny members of "3010s";

            #filename "PV_10md-2mu_cpe3_DOC11.bin";
            filename "P2_TOS_DOC11.bin";
            
            default-lease-time 1200;
            max-lease-time 1200;
            range 10.160.248.21 10.160.251.250;
        }
    }

include "dhcp3/cmts/host_includes/ilcudevubr.hosts";

    subnet 66.253.201.144 netmask 255.255.255.240 {
        option routers 66.253.201.145;
        option subnet-mask 255.255.255.240;
        option broadcast-address 66.253.201.159;
        option domain-name "client.pavlovmedia.net";
        option netbios-name-servers 66.253.230.70;
	option domain-name-servers 66.253.214.16, 50.30.184.16;
#        default-lease-time dTlease0;
#        max-lease-time mdTlease0;
        pool {
            deny members of "ruckus";
            deny members of "cm-unregistered";
            deny members of "3010s";
            range 66.253.201.146 66.253.201.155;
        }
    }
}
