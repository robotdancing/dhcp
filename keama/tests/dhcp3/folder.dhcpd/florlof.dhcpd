# florlof DHCPD Config File.
# Written 2010-12-15-18:10 by mroberts.

shared-network florlof {
    authoritative;

#  include "/etc/dhcp3/folder.dhcpd/events.dhcpd";

    subnet 66.158.229.26 netmask 255.255.255.255 {
    }
    #dhcpd-snmp=florlof-cust0,10.16.4.20-10.16.7.200
    subnet 10.16.4.0 netmask 255.255.252.0 {
        option routers 10.16.4.1;
        option subnet-mask 255.255.252.0;
        option broadcast-address 10.16.7.255;
        option domain-name "client.pavlovmedia.net";
        option domain-name-servers 66.253.214.16, 50.30.184.16;
        default-lease-time 14400;
        max-lease-time 14400;
        pool {
            failover peer "NATIONAL";
            deny members of "ruckus";
            deny members of "ubnt";
            deny dynamic bootp clients;
            range 10.16.4.20 10.16.7.200;
        }
    }

  
    #dhcpd-snmp=florlof-ruckus,10.153.30.20-10.153.30.200
    subnet 10.153.30.0 netmask 255.255.255.0 {
        option routers 10.153.30.1;
        option subnet-mask 255.255.255.0;
        option broadcast-address 10.153.30.255;
        pool {
            failover peer "NATIONAL";
            allow members of "ruckus";
                allow members of "ubnt";
            deny dynamic bootp clients;
            default-lease-time 7220;
            max-lease-time 7220;
            range 10.153.30.20 10.153.30.200;
        }

    }
}

