{
  # string option-data with embedded commas
  # authoritative is mandatory
  "Dhcp4": {
    "option-def": [
      # vendor option space
      {
        "space": "dhcp4",
        "name": "a-string",
        "code": 250,
        "type": "string"
      }
    ],
    "option-data": [
      {
        "space": "dhcp4",
        "name": "a-string",
        "code": 250,
        "data": "foo\\, bar"
      }
    ]
  }
}
