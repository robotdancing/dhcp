{
  # DHCPv4 range config
  # authoritative is mandatory
  "Dhcp4": {
    "subnet4": [
      # subnet declaration
      {
        "subnet": "10.5.5.0/27",
        "option-data": [
          # range declaration
          {
            "space": "dhcp4",
            "name": "domain-search",
            "code": 119,
            "data": "example.com, example.org"
          }
        ],
        "valid-lifetime": 1800,
        "pools": [
          {
            "pool": "10.5.5.5 - 10.5.5.10"
          }
        ]
      }
    ]
  }
}
