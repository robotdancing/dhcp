{
  # DHCPv4 permit authenticated client config
  # authoritative is mandatory
  "Dhcp4": {
    "valid-lifetime": 1800,
    "subnet4": [
      # subnet declaration
      {
        "subnet": "10.5.5.0/27",
        "pools": [
          # pool declaration
          {
            # avoid empty pool
            "pool": "10.5.5.5 - 10.5.5.10"
//          "allow": [
//            "authenticated clients"
//          ]
          }
        ]
      }
    ]
  }
}
