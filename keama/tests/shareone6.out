{
  # DHCPv6 one-subnet shared-network declaration config
  # authoritative is mandatory
  "Dhcp6": {
    "interfaces-config": {
      "interfaces": [
        "en0"
      ]
    },
    "subnet6": [
      # subnet declaration
      {
        "subnet": "2001::/64",
        "valid-lifetime": 3600,
        "pools": [
          {
            # pool
            "pool": "2001::1000 - 2001::2000"
          }
        ],
        "interface": "en0",
        "option-data": [
          # interface
          # option
          {
            "space": "dhcp6",
            "name": "domain-search",
            "code": 24,
//          "original-data": "\"example.com\", \"example.org\"",
            "data": "example.com, example.org"
          }
        ]
      }
    ]
  }
}
