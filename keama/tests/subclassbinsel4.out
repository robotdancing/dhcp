{
  # subclass declaration config
  # authoritative is mandatory
  "Dhcp4": {
    "option-def": [
      # options
      {
        "space": "dhcp4",
        "name": "myversion",
        "code": 251,
        "type": "uint16"
      }
    ],
    "client-classes": [
      # superclass declaration
      {
        "name": "foobar",
//      "spawning": false,
//      "submatch": {
//        "hardware": null
