{
  # DHCPv4 pool with 2 ranges config
  # authoritative is mandatory
  "Dhcp4": {
    "subnet4": [
      # subnet declaration
      {
        "subnet": "10.5.5.0/27",
        "option-data": [
          {
            "space": "dhcp4",
            "name": "domain-search",
            "code": 119,
//          "original-data": "\"example.com\", \"example.org\"",
            "data": "example.com, example.org"
          }
        ],
        /// valid-lifetime moved from an internal pool scope
        "valid-lifetime": 1800,
        "pools": [
          {
            "pool": "10.5.5.5 - 10.5.5.10"
          },
          # pool declaration
          {
            # add a second range
            "pool": "10.5.5.11 - 10.5.5.12"
          }
        ],
        "interface": "foo"
      }
    ],
    "interfaces-config": {
      "interfaces": [
        "foo"
      ]
    }
  }
}
