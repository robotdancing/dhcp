{
  # embedded option-data in DHCPv4 pool config
  # authoritative is mandatory
  "Dhcp4": {
    "valid-lifetime": 1800,
    "subnet4": [
      # subnet declaration
      {
        "subnet": "10.5.5.0/27",
        "pools": [
          # pool declaration
          {
            # avoid empty pool
            "pool": "10.5.5.5 - 10.5.5.10"
          }
        ]
      }
    ]
  }
}
